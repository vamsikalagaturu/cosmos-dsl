{
    "@context": [
        "../contexts/entities/point.jsonld",
        "../contexts/relations/spatial_relations.jsonld",
        {
            "rob": "http://example.com/rob#"
        }
    ],
    
    "@id": "rob:pos_instance",

    "@graph": [
        {
            "point": "rob:p1"
