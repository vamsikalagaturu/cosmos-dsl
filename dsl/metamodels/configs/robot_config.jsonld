{
    "@context": {
        "robotConfig": "http://example.com/robot_config#",

        "RobotConfig": "robotConfig:RobotConfig",

        "urdf-name": {
            "@id": "robotConfig:urdf-name",
            "@type": "xsd:string"
        },

        "initial-state": {
            "@id": "robotConfig:initial-state",
            "@container": "@list",
            "@type": "xsd:double"
        },

        "base-frame": {
            "@id": "robotConfig:base-frame",
            "@type": "@type"
        },

        "tool-frame": {
            "@id": "robotConfig:tool-frame",
            "@type": "@type"
        }
    }
}