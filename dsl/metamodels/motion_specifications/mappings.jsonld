{
    "@context": {
        "mapping": "http://example.com/mappings#",

        "ControllerSolver": "mapping:ControllerSolver",
        "AccEnegery": "mapping:AccEnegery",

        "interface": {
            "@id": "mapping:interface",
            "@type": "@id"
        },

        "controller": {
            "@id": "mapping:controller",
            "@type": "@id"
        },

        "solver": {
            "@id": "mapping:solver",
            "@type": "@id"
        },

        "controller-input": {
            "@id": "mapping:controller-input",
            "@type": "@id"
        },

        "controller-input-dimension": {
            "@id": "mapping:controller-input-dimension",
            "@type": "xsd:string",
            "@container": "@set"
        },

        "controller-output": {
            "@id": "mapping:controller-output",
            "@type": "@id"
        },

        "controller-output-dimension": {
            "@id": "mapping:controller-output-dimension",
            "@type": "xsd:int",
            "@container": "@list"
        }
    }
}