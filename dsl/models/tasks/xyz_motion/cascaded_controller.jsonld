{
    "@context": [
        "../../../metamodels/controllers/cascaded_controller.jsonld",
        "../../../metamodels/controllers/pid_controller.jsonld",
        {
            "rob": "http://example.com/rob#"
        }
    ],
    "@id": "rob:cascaded_controllers",
    "@graph": [
        {
            "@id": "rob:pid_position_controller",
            "@type": "PIDController",
            "p-gain": 5.0,
            "i-gain": 0.9,
            "d-gain": 0.0,
            "time-step": 0.01,
            "constraint": "rob:bracelet_link-target_link-constraint_dist"
        },
        {
            "@id": "rob:pid_velocity_controller",
            "@type": "PIDController",
            "p-gain": 20.0,
            "i-gain": 0.9,
            "d-gain": 0.0,
            "time-step": 0.1,
            "constraint": "rob:bracelet_link-target_link-constraint_vel"
        },
        {
            "@id": "rob:cascaded_controller",
            "@type": ["CascadedController", "PositionVelocityController"],
            "position-controller": "rob:pid_position_controller",
            "velocity-controller": "rob:pid_velocity_controller"
        }
    ]
}