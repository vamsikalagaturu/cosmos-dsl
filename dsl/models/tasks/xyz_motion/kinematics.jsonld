{
    "@context": [
        "../../../metamodels/relations/kinematics.jsonld",
        "https://comp-rob2b.github.io/metamodels/qudt.json",
        {
            "rob": "http://example.com/rob#"
        }
    ],
    "@id": "rob:distance_instance",
    "@graph": [
        {
            "@id": "rob:dist-bracelet_link-target_link",
            "@type": [
                "EuclideanDistance",
                "FramePointToPointDistance"
            ],
            "between-entities": [
                "rob:bracelet_link",
                "rob:target_link"
            ],
            "quantity-kind": "Length"
        },
        {
            "@id": "rob:vel-bracelet_link-target_link",
            "@type": [
                "CartesianVelocity",
                "LinearVelocity"
            ],
            "of-frame": "rob:bracelet_link",
            "wrt-frame": "rob:target_link",
            "quantity-kind": "Velocity"
        }
    ]
}