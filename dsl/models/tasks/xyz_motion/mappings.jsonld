{
    "@context": [
        "../../../metamodels/motion_specifications/controller_io.jsonld",
        "../../../metamodels/motion_specifications/controller_mappings.jsonld",
        "../../../metamodels/motion_specifications/mappings.jsonld",
        {
            "rob": "http://example.com/rob#"
        }
    ],
    "@id": "rob:motion_specifications",
    "@graph": [
        {
            "@id": "rob:pid_velocity_controller_io_input",
            "@type": [
                "ControllerIO",
                "ControllerInput"
            ],
            "io": "rob:bracelet_link_coord",
            "io-dimension": [1, 1, 1, 0, 0, 0]
        },
        {
            "@id": "rob:pid_velocity_controller_io_output",
            "@type": [
                "ControllerIO",
                "ControllerOutput"
            ],
            "io-dimension": [1, 1, 1, 0, 0, 0]
        },
        {
            "@id": "rob:pid_velocity_controller_mappings",
            "@type": "ControllerMapping",
            "controller": "rob:pid_velocity_controller",
            "input": "rob:pid_velocity_controller_io_input",
            "output": "rob:pid_velocity_controller_io_output"
        },
        {
            "@id": "rob:controller_solver_mappings",
            "@type": "ControllerSolver",
            "interface": {
                "@id": "rob:acc_energy",
                "@type": "AccEnegery"
            },
            "controller-mappings": ["rob:pid_velocity_controller_mappings"],
            "solver": "rob:vereshchagin_solver",
            "solver-input": "rob:pid_velocity_controller_io_output"
        }
    ]
}